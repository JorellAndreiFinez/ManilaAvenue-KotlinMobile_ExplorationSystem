<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >
    <corners
        android:topLeftRadius="50dp"
        android:topRightRadius="50dp"
        android:bottomLeftRadius="50dp"
        android:bottomRightRadius="50dp"
        />
    <gradient
        android:angle="45"
        android:centerX="35%"
        android:startColor="@color/LightKhaki"
        android:endColor="@color/LightKhaki"
        android:type="linear"
        />
    <padding
        android:left="10dp"
        android:top="0dp"
        android:right="10dp"
        android:bottom="0dp"
        />
    <size
        android:width="50dp"
        android:height="50dp"
        />
    <stroke
        android:width="2dp"
        android:color="@color/LightBlack"
        />
</shape>